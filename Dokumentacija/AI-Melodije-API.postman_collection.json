	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"const jwtUrl = pm.collectionVariables.get(\"bazican_url\") + \"/auth/token\";",
					"",
					"pm.sendRequest({",
					"    url: jwtUrl,",
					"    method: 'GET',",
					"    header: {",
					"        'Content-type': 'application/json'",
					"    }",
					"}, (err, res) => {",
					"    console.log(res.json().token);",
					"    pm.collectionVariables.set(\"jwt\", res.json().token);",
					"});"
				]
			}
		},
